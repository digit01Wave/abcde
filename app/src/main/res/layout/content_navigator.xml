<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.jessica.myuci.NavigatorActivity"
    tools:showIn="@layout/activity_navigator">



    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EventList"
        android:id="@+id/button3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:onClick="goEventListActivity"/>


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/krumbs_button_label"
        android:id="@+id/kcapturebutton"
        android:layout_below="@+id/button3"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true"
        android:onClick="goKrumbsActivity"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Watch Later"
        android:id="@+id/watch_later_list_button"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/kcapturebutton"
        android:layout_centerHorizontal="true"
        android:onClick="goWatchLaterListActivity"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calendar"
        android:id="@+id/calendar_button"
        android:layout_below="@+id/watch_later_list_button"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:onClick="goCalendarActivity"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Current List"
        android:id="@+id/current_button"
        android:layout_below="@+id/calendar_button"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:onClick="goToCurrentListActivity"/>


</RelativeLayout>
